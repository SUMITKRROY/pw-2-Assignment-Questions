import java.util.Scanner;

public class create_test_case {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int t = sc.nextInt();
        int n = sc.nextInt();
        int arr[][]=new int[t][n];
        for (int i=0;i<t; i++){
            for (int j=0;j<n;j++){
                arr[i][j]=sc.nextInt();
            }
        }
        int result=0;
        for (int i=0;i<t; i++){
            for (int j=0;j<n;j++){
                result +=arr[i][j];
            }
            System.out.println(result);
            result=0;
        }

    }
}
